<script>
    $(document).ready(function(e) {
        $("#return").click(function(){
            $('body,html').animate({scrollTop:0},500);
        })
        $('.flashn').height(oWidht*404/1440);
        $('.flashn img').height(oWidht*404/1440);
        //hd

        $(window).scroll( function(){
            var oHeight = $(window).height();
            var oWidht = $(window).width();
            var oScrolltop = $(document).scrollTop();
            //$(document).attr('title',oScrolltop)

            if(oScrolltop>$('.contact').offset().top-500){
                $('.contact .left').animate({left:oWidht/2-520},1000);
                $('.contact .right').animate({right:oWidht/2-520},1000);
                var i =0;
                //$("#ul1 li:eq(0)").css({display:"none"})
                var t = $("#ul1 li").length;
                var oTime = setInterval(function(){
                    $("#ul1 li").eq(i > t-1 ? i = 0 : i++).animate({bottom:440-(i)*24},1000);
                },500)
                setTimeout(function(){ clearInterval(oTime)},t*500+300)
            }
            //tk
            if(oScrolltop>$('.feedback').offset().top-600){

                $('.feedback .left').animate({left:oWidht/2-520},1000);
                $('.feedback .right').animate({right:oWidht/2-520},1000);
            }
        })

        //btn
        $('.btn ul li:eq(0)').click(function(){
            $("html,body").animate({scrollTop:$('.contact').offset().top-125},1000)
        })

        $('.btn ul li:eq(1)').click(function(){
            $("html,body").animate({scrollTop:$('.feedback').offset().top-125},1000)
        })

        $('.btn ul li:eq(2)').click(function(){
            $("html,body").animate({scrollTop:$('.sitemap').offset().top-125},1000)
        })
        //ment
        $(window).resize(function(){location.replace(location) })
    });
</script>
